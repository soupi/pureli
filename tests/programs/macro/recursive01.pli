(module main)

(define cond ~params
  (if (empty? 'params)
      (error "*** error: cond - non-exhuastive patterns")
      (if (eval (car (car 'params)))
          (eval (car (cdr (car 'params))))
          (cond (cdr 'params)))))

(define main
  (do!
    [print! x]))

(define x
  (cond (#f (error "error"))
        ((= 1 2) 2)
        ((= 3 3) 3)
        (#t "else")))
